    "express": "~4.15.5",
    "knex": "^0.14.4",
    "morgan": "~1.9.0",
    "nodemon": "^1.15.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "pg": "^7.4.1",
    "pryjs": "^1.0.3",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
